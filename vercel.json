{
    "routes": [
       
        {
            "src": "/sitemap.xml",
            "dest": "/sitemap.xml",
            "headers": {
                "Cache-Control": "public, max-age=3600, s-maxage=3600",
                "Content-Type": "application/xml"
            }
        },
        {
            "src": "/robots.txt",
            "dest": "/robots.txt",
            "headers": {
                "Cache-Control": "public, max-age=3600, s-maxage=3600",
                "Content-Type": "text/plain"
            }
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/images/(.*)",
            "dest": "/images/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/asset-manifest.json",
            "dest": "/asset-manifest.json"
        },
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
